{
  "name": "AllerMedia",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "start": "react-native start",
    "test": "jest",
    "postinstall": "patch-package",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "asset:reset": "react-native-asset",
    "android:pre_build": "react-native bundle --platform android --dev false --entry-file index.js --bundle-output android/app/src/main/assets/index.android.bundle",
    "android:pre_build-img": "react-native bundle --platform android --dev false --entry-file index.js --bundle-output android/app/src/main/assets/index.android.bundle --assets-dest android/app/src/main/res",
    "android:clean": "cd android/ && ./gradlew clean && cd .. && react-native run-android",
    "android:connect": "adb reverse tcp:8081 tcp:8081",
    "android:build_apk": "cd android && ./gradlew clean && ./gradlew assembleRelease && cd ..",
    "android:build_bundle": "cd android && ./gradlew clean && ./gradlew bundleRelease && cd ..",
    "android:sdk-fix": "rm -rf android/.gradle && rm -rf .gradle && cd android && rm -rf ~/.gradle && ./gradlew clean",
    "ios:build": "react-native bundle --entry-file='index.js' --bundle-output='./ios/main.jsbundle' --dev=false --platform='ios'",
    "ios:build-img": "react-native bundle --entry-file ./index.js --platform ios --bundle-output ios/main.jsbundle --assets-dest ios",
    "ios:simulator": "npx react-native run-ios --simulator 'iPhone 14'",
    "ios:simulator2": "npx react-native run-ios --simulator 'iPhone SE (3rd generation)'",
    "ios:simulator-iPad": "npx react-native run-ios --simulator 'iPad Pro (12.9-inch) (5th generation)'",
    "ios:device": "npx react-native run-ios --device",
    "reset-cache": "watchman watch-del-all && react-native start --reset-cache"
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "^1.17.11",
    "@react-navigation/native": "^6.1.6",
    "@react-navigation/native-stack": "^6.9.12",
    "@reduxjs/toolkit": "^1.9.3",
    "patch-package": "^6.5.1",
    "react": "18.2.0",
    "react-native": "0.71.4",
    "react-native-paper": "^5.3.1",
    "react-native-safe-area-context": "^4.5.0",
    "react-native-screens": "^3.20.0",
    "react-native-vector-icons": "^9.2.0",
    "react-native-webview": "^11.26.1",
    "react-redux": "^8.0.5",
    "redux": "^4.2.1",
    "redux-persist": "^6.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@babel/preset-env": "^7.20.0",
    "@babel/runtime": "^7.20.0",
    "@react-native-community/eslint-config": "^3.2.0",
    "@tsconfig/react-native": "^2.0.2",
    "@types/jest": "^29.2.1",
    "@types/react": "^18.0.24",
    "@types/react-test-renderer": "^18.0.0",
    "@typescript-eslint/eslint-plugin": "^5.40.0",
    "@typescript-eslint/parser": "^5.40.0",
    "babel-jest": "^29.2.1",
    "babel-plugin-module-resolver": "^4.1.0",
    "eslint": "^8.25.0",
    "eslint-import-resolver-typescript": "^3.5.1",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-no-inline-styles": "^1.0.5",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.31.10",
    "eslint-plugin-react-hooks": "^4.6.0",
    "husky": "^8.0.2",
    "jest": "^29.2.1",
    "lint-staged": "^13.0.4",
    "metro-react-native-babel-preset": "0.73.8",
    "prettier": "^2.7.1",
    "react-native-svg-transformer": "^1.0.0",
    "react-native-typescript-transformer": "^1.2.13",
    "react-test-renderer": "18.2.0",
    "typescript": "4.8.4"
  },
  "jest": {
    "preset": "react-native"
  }
}
